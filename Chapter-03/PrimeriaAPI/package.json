{
  "name": "rentx",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Bruno Ferrari <kanedaferrari@yahoo.com.br>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules --respawn src/index.ts",
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./src/database/index.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d ./typeOrm.config.ts migration:generate ./migrations/$npm_config_name",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./migrations/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./typeOrm.config.ts migration:revert"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "csv-parse": "^5.3.3",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.6.0",
    "tsyringe": "4.4.0",
    "typeorm": "0.3.11",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.10",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "multer": "^1.4.5-lts.1",
    "prettier": "^2.8.3",
    "ts-node": "10.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.5.2"
  }
}
